{
  "name": "arcinfer-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "oxlint src/",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint",
    "build:static": "STATIC_EXPORT=true next build && rm -rf out/models && find out/onnx -type f ! -name 'ort-wasm-simd-threaded.jsep.wasm' -delete",
    "deploy": "npm run build:static && npx wrangler pages deploy out --project-name arcinfer"
  },
  "dependencies": {
    "@arcium-hq/client": "^0.8.3",
    "@arcium-hq/reader": "^0.8.3",
    "@coral-xyz/anchor": "^0.32.1",
    "@huggingface/transformers": "^3.8.1",
    "@noble/curves": "^1.8.0",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.4",
    "lucide-react": "^0.564.0",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^22.15.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "oxlint": "^1.47.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  }
}
